<%- include("partials/header"); %>
<div class="main">
  <p class="result-stats"> <span>About <%=resultStats.noOfResults%> </span> results<span> ( <%=resultStats.time%> seconds)</span> </p>
  <%=searchedWord.noOfResults%>
  <div class="players">
    <% players.forEach(player=>{%>
    <div class="player">
        <img class="player__img" src=<%=player.photo!="null"?""+player.photo:"images/default-picture.png"%> alt="image">
      <p> <%=player.full_name%></p>
    </div>
  <%})%>
  </div>

  </div>
  <%- include("partials/footer"); %>